<h1>Posts</h1>
<% @posts.each do |post|%>
	<h2><%= link_to post.title, post %></h2>
	<p><%= post.created_at.strftime("%d %B %Y")%></p>
	<p>Post by: <%= post.user.username %></p>
	<p>Category:<%= link_to post.category.name, category_posts_path(post.category) %> </p>

	
	
	
	<p><%= post.body %></p>
<% end %>